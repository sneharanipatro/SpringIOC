<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation=" http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd
	http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">


<context:annotation-config/>
<bean id="humanheart" class="com.sneha.autowired.Heart">
<property name="nameOfAnimal" value="Human"/>
<property name="noOfHeart" value="1"/>
</bean>
<bean id="octpousheart" class="com.sneha.autowired.Heart">
<property name="nameOfAnimal" value="Octpous"/>
<property name="noOfHeart" value="3"/>
</bean>
