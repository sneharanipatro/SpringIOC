<bean id="human" class="com.sneha.autowired.Human" autowire="byType"></bean>
<bean id="human" class="com.sneha.autowired.Human" autowire="constructor"></bean>-->
<bean id="human" class="com.sneha.autowired.Human"></bean>
</beans>